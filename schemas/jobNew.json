{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Photographer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": "",
            "title": "The salary Schema",
            "examples": [
                "200000"
            ]
        },
        "equity": {
            "type": "string",
            "default": "",
            "title": "The equity Schema",
            "examples": [
                "0"
            ]
        },
        "company_handle": {
            "type": "string",
            "default": "",
            "title": "The company_handle Schema",
            "examples": [
                "davis-davis"
            ]
        }
    },
    "examples": [{
        "title": "Photographer",
        "salary": "200000",
        "equity": "0",
        "company_handle": "davis-davis"
    }]
}